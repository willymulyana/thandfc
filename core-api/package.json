{
  "name": "@tandf/core-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.production.json && tsc-alias",
    "watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "clean": "rimraf -rf ./dist",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "ts:check": "tsc --noEmit",
    "tidy": "eslint . --ext .js,.ts,.tsx --fix",
    "test:unit": "jest ./src",
    "test:e2e": "jest ./test/e2e",
    "db:start": "docker-compose up",
    "dev": "nodemon dist/src/index.js",
    "start": "NODE_ENV=production node dist/src/index.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "seed": "node dist/src/database/seed/index.js",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --transaction=each"
  },
  "dependencies": {
    "@apollo/server": "^4.0.1",
    "@faker-js/faker": "^7.6.0",
    "@jest/types": "~29.2.0",
    "@types/supertest": "^2.0.12",
    "body-parser": "^1.20.1",
    "class-validator": "^0.13.2",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "~3.0.2",
    "supertest": "^6.3.0",
    "tsc-alias": "^1.7.0",
    "tslib": "^2.0.3",
    "type-graphql": "^2.0.0-beta.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.44",
    "typeorm-typedi-extensions": "^0.2.3",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1",
    "@types/cors": "^2",
    "@types/express": "^4",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.5",
    "@types/pg": "^8",
    "@types/yargs": "^17",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^7.32.0",
    "jest": "~29.2.0",
    "ts-jest": "~29.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5"
  }
}
